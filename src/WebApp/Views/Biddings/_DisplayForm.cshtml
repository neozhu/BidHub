﻿@model WebApp.Models.Bidding
<!-- begin master section -->
<div class="row">
    <div class="col col-lg-12 col-md-12">
<!--begin bidding form  -->
<form id="bidding_form" class="smart-form" method="post" novalidate >
@Html.AntiForgeryToken()
  <input type="hidden" id="@Html.IdFor(model => model.Id)" 
                       name="@Html.NameFor(model => model.Id)" 
                       value="@Html.ValueFor(model => model.Id)" />
 <fieldset>
     <div class="row">
			              <!--投标时间-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.BiddingDate)</span> </label>
            <label class="label">
            
			@Html.DisplayFor(model => model.BiddingDate)
			    </label>
            </section>
			  		 
	    
			              <!--状态-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.Status)</span> </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Status)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			              <!--采购单号-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.PO)</span> </label>
            <label class="label">
			 @Html.DisplayFor(model => model.PO)
            </label>
            </section>
 		 
	    
			              <!--要求到货日期-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.DemandedDate) </label>
            <label class="label">
            
			@Html.DisplayFor(model => model.DemandedDate)
			    </label>
            </section>
			  		 
	   </div>
	    
     <div class="row">
			              <!--序号-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.LineNum)</span> </label>
            <label class="label">
			 @Html.DisplayFor(model => model.LineNum)
            </label>
            </section>
 		 
	    
			              <!--货号-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.ProductNo) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.ProductNo)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			              <!--品名-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.ProductName)</span> </label>
            <label class="label">
			 @Html.DisplayFor(model => model.ProductName)
            </label>
            </section>
 		 
	    
			              <!--规格-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.Spec) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Spec)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			              <!--品牌-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.BrandName) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.BrandName)
            </label>
            </section>
 		 
	    
			              <!--单位-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.Unit) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Unit)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			  			<!--数量-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.Qty)</span> </label>
            <label class="label">
            @Html.DisplayFor(model => model.Qty)
			    </label>
            </section>
		 
	    
			  			<!--出价-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.BiddingPrice)</span> </label>
            <label class="label">
            @Html.DisplayFor(model => model.BiddingPrice)
			    </label>
            </section>
		 
	   </div>
	    
     <div class="row">
			  			<!--总价-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.TotalPrice)</span> </label>
            <label class="label">
            @Html.DisplayFor(model => model.TotalPrice)
			    </label>
            </section>
		 
	    
			              <!--参数-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.Feature) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Feature)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			              <!--备注-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.Description) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Description)
            </label>
            </section>
 		 
	    
			              <!--业务联系人-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.Buyer) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.Buyer)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			              <!--联系人电话-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.BuyerPhone) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.BuyerPhone)
            </label>
            </section>
 		 
	    
			              <!--出价人-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.UserName) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.UserName)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			  			<!--供应商ID-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.SupplierId)</span> </label>
            <label class="label">
            @Html.DisplayFor(model => model.SupplierId)
			    </label>
            </section>
		 
	    
			              <!--供应商-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.SupplierName) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.SupplierName)
            </label>
            </section>
 		 
	   </div>
	    
     <div class="row">
			              <!--询价单号-->
            <section class="col col-6">
            <label class="label">@Html.DisplayNameFor(model => model.DocNo) </label>
            <label class="label">
			 @Html.DisplayFor(model => model.DocNo)
            </label>
            </section>
 		 
	    
            <!--采购单-->
            <section class="col col-6">
            <label class="label"><span class="required">@Html.DisplayNameFor(model => model.PurchaseOrderId)</span> </label>
            <label class="label">
			      @Html.DisplayFor(model => model.PurchaseOrderId)
			</label>
            </section>
			 
	   </div>
	    
	    
</fieldset>
</form>
<!-- end bidding form  -->
</div>
</div>
<!-- end master section -->
<!--========================================================================================-->


